{% extends 'layout.html.twig' %}

{% block title %}
    Años
{% endblock %}

{% block content %}
    <h1 class="titulo">Elija el año</h1>
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <a href="{{ path('registro_listar') }}" class="btn btn-info"><i class="fa fa-arrow-left"></i> Volver al listado</a>
    <button type="submit" class="btn btn-danger"><i class="fa fa-trash"></i> Eliminar los registros</button>
    {{ form_end(form) }}
{% endblock %}